    "version": "0.2.0",
    "configurations": [
      {
        "name": "Firefox Debug",
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "url": "http://localhost:3000/",
        "webRoot": "${workspaceFolder}",
        "pathMappings": [
          {
            "url": "webpack:///api/models",
            "path": "${workspaceFolder}/api/routes"
          },
          {
            "url": "webpack:///api/models",
            "path": "${workspaceFolder}/api/routes"
          },
          {
            "url": "webpack:///api/routes",
            "path": "${workspaceFolder}/api/routes"
          },
          {
            "url": "webpack:///api/models",
            "path": "${workspaceFolder}/api/models"
          },
          {
            "url": "webpack:///requests",
            "path": "${workspaceFolder}/requests"
          },
          {
            "url": "webpack:///components",
            "path": "${workspaceFolder}/components"
          }
        ]
      }
    ]
}
